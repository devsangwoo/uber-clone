{
	"extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
	"rules": {
	  "max-classes-per-file": false,
	  "jsx-no-lambda": false,
	  "no-console": false,
	  "no-empty-interface": false
	},
	"linterOptions": {
	  "exclude": [
		"config/**/*.js",
		"node_modules/**/*.ts",
		"coverage/lcov-report/*.js",
		"src/utils/countries.ts"
	  ]
	}
  }
