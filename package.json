      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-facebook-login": "^4.1.1",
    "@types/react-sidebar": "^3.0.0",
    "husky": "^4.0.7",
    "react-facebook-login": "^4.1.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0"
